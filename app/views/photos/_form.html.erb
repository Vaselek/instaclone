<%= simple_form_for @photo, :html => {:multipart => true} do |f| %>
	<%= render 'shared/error_messages', object: f.object %>
	<div>
		<%= f.input :title, label: 'Description of photo' %>
	</div><br/>
	<div>
		<%= f.file_field :image %>
	</div><br/>
	<div>
		<%= f.button :submit, class: "btn btn-default btn-primary" %>
	</div>
<% end %>

